  "workbench.startupEditor": "newUntitledFile",
  "macros": {
    "copyCommentLine": [
      "editor.action.addCommentLine",
      "editor.action.copyLinesDownAction",
      "editor.action.removeCommentLine",
    ],
    "endwithsemicolon": [
      "cursorLineEnd",
      "my_snippet",
      {
        "command": "editor.action.insertSnippet",
        "args": {
          "snippet": ";"
        }
      }
    ],
    "surroundWordSingleQuotes": [
      "editor.action.addSelectionToNextFindMatch",
      "my_snippet",
      {
        "command": "editor.action.insertSnippet",
        "args": {
          "snippet": "'$TM_SELECTED_TEXT$1'$0"
        }
      }
    ],
    "surroundWordQuotes": [
      "editor.action.addSelectionToNextFindMatch",
      "my_snippet",
      {
        "command": "editor.action.insertSnippet",
        "args": {
          "snippet": "\"$TM_SELECTED_TEXT$1\"$0"
        }
      }
    ],
    "surroundWordParens": [
      "editor.action.addSelectionToNextFindMatch",
      "my_snippet",
      {
        "command": "editor.action.insertSnippet",
        "args": {
          "snippet": "($TM_SELECTED_TEXT$1)$0"
        }
      }
    ],
    "surroundWordBrackets": [
      "editor.action.addSelectionToNextFindMatch",
      "my_snippet",
      {
        "command": "editor.action.insertSnippet",
        "args": {
          "snippet": "[$TM_SELECTED_TEXT$1]$0"
        }
      }
    ],
    "surroundWordCurly": [
      "editor.action.addSelectionToNextFindMatch",
      "my_snippet",
      {
        "command": "editor.action.insertSnippet",
        "args": {
          "snippet": "{$TM_SELECTED_TEXT$1}$0"
        }
      }
    ]
  },
