{
  // Place your settings in this file to overwrite the default settings
  "color-highlight.markerType": "outline",
  "editor.cursorBlinking": "phase",
  // "editor.cursorStyle": "underline-thin",
  "editor.cursorStyle": "line",
  "editor.fontSize": 21,
  // "editor.fontFamily": "OperatorMono-Book",
  "editor.fontFamily": "OperatorMonoLig-Book",
  // "editor.fontFamily": "OperatorMonoLig-Medium",
  "editor.tabSize": 2,
  "editor.tabCompletion": false,
  "editor.fontLigatures": true,
  // "editor.formatOnSave": true,
  "editor.minimap.enabled": true,
  "editor.minimap.maxColumn": 80,
  "editor.minimap.showSlider": "always",
  "editor.occurrencesHighlight": false,
  "editor.rulers": [
    80
  ],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "markdown": "html",
    "go": "html",
    "python": "html",
    "ejs": "html",
  },
  "emmet.excludeLanguages": [],
  "editor.wordWrap": "bounded",
  // "files.autoSave": "onFocusChange",
  "html.format.enable": false,
  "guides.normal.style": "dotted",
  "prettier.eslintIntegration": true,
  "python.terminal.executeInFileDir": true,
  "terminal.integrated.fontSize": 21,
  "vim.cursorStylePerMode": {
    // "normal": "underline"
    // "normal": "underline-thin",
    // "visual": "underline-thin",
    // "visualline": "underline-thin"
    // "insert": "line"
  },
  "vim.easymotion": true,
  "vim.easymotionMarkerFontSize": "20",
  "vim.easymotionMarkerWidthPerChar": 14,
  "vim.easymotionMarkerHeight": 27,
  "vim.easymotionMarkerYOffset": 20,
  "vim.easymotionKeys": "hklyuiopnmqwertzxcvbasdgjf",
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "f",
        "d"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "d"
      ],
      "after": [
        "d",
        "d"
      ]
    },
    {
      "before": [
        "<leader>",
        "a"
      ],
      "after": [
        "e",
        "a"
      ]
    },
    {
      "before": [
        "<leader>",
        "i"
      ],
      "after": [
        "b",
        "i"
      ]
    },
    {
      "before": [
        "j"
      ],
      "after": [
        "g",
        "j"
      ]
    },
    {
      "before": [
        "k"
      ],
      "after": [
        "g",
        "k"
      ]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-e>": false
  },
  // "workbench.iconTheme": "vscode-icons",
  "window.zoomLevel": 0,
  "window.openFilesInNewWindow": "off",
  // "window.title": "${activeEditorShort}${separator}${folderName}",
  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
  // Configure editor settings to be overridden for [markdown] language.
  "[javascript]": {
    "editor.tabSize": 4,
    "editor.formatOnSave": true,
  },
  "[typescript]": {
    "editor.tabSize": 4,
    "editor.formatOnSave": true,
  },
  "[html]": {
    "editor.wordWrapColumn": 70,
    "editor.formatOnSave": true,
  },
  "[markdown]": {
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 60,
    "editor.quickSuggestions": true,
    "editor.lineNumbers": "off",
  },
  "[latex]": {
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 60,
    "editor.quickSuggestions": true,
    "editor.minimap.enabled": false
  },
  "[php]": {
    "editor.tabSize": 4
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "[go]": {
    "editor.tabSize": 4
  },
  "workbench.startupEditor": "newUntitledFile",
  "macros": {
    "copyCommentLine": [
      "editor.action.addCommentLine",
      "editor.action.copyLinesDownAction",
      "editor.action.removeCommentLine",
    ],
    "endwithsemicolon": [
      "cursorLineEnd",
      "my_snippet",
      {
        "command": "editor.action.insertSnippet",
        "args": {
          "snippet": ";"
        }
      }
    ],
    "surroundWordSingleQuotes": [
      "editor.action.addSelectionToNextFindMatch",
      "my_snippet",
      {
        "command": "editor.action.insertSnippet",
        "args": {
          "snippet": "'$TM_SELECTED_TEXT$1'$0"
        }
      }
    ],
    "surroundWordQuotes": [
      "editor.action.addSelectionToNextFindMatch",
      "my_snippet",
      {
        "command": "editor.action.insertSnippet",
        "args": {
          "snippet": "\"$TM_SELECTED_TEXT$1\"$0"
        }
      }
    ],
    "surroundWordParens": [
      "editor.action.addSelectionToNextFindMatch",
      "my_snippet",
      {
        "command": "editor.action.insertSnippet",
        "args": {
          "snippet": "($TM_SELECTED_TEXT$1)$0"
        }
      }
    ],
    "surroundWordBrackets": [
      "editor.action.addSelectionToNextFindMatch",
      "my_snippet",
      {
        "command": "editor.action.insertSnippet",
        "args": {
          "snippet": "[$TM_SELECTED_TEXT$1]$0"
        }
      }
    ],
    "surroundWordCurly": [
      "editor.action.addSelectionToNextFindMatch",
      "my_snippet",
      {
        "command": "editor.action.insertSnippet",
        "args": {
          "snippet": "{$TM_SELECTED_TEXT$1}$0"
        }
      }
    ]
  },
  "workbench.colorTheme": "Night Owl",
  "workbench.iconTheme": "material-icon-theme",
  "files.eol": "\n",
  "files.associations": {
    "*.fish": "fish",
    "*.gohtml": "html",
    "*.php": "php"
  },
  "spellright.language": "english",
  "workbench.fontAliasing": "antialiased",
  "go.autocompleteUnimportedPackages": true,
  "go.useCodeSnippetsOnFunctionSuggest": true,
  "latex-workshop.latex.clean.fileTypes": [
    "*.aux",
    "*.bbl",
    "*.blg",
    "*.idx",
    "*.ind",
    "*.lof",
    "*.lot",
    "*.out",
    "*.toc",
    "*.acn",
    "*.acr",
    "*.alg",
    "*.glg",
    "*.glo",
    "*.gls",
    "*.ist",
    "*.fls",
    "*.log",
    "*.fdb_latexmk",
    "*.nav",
    "*.snm",
    "*.synctex.gz"
  ],
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.snippetSuggestions": "top",
  "editor.formatOnPaste": true,
  "rust.mode": "rls",
  "rust.rustup": {
    "toolchain": "stable-x86_64-apple-darwin",
    "nightlyToolchain": "nightly-x86_64-apple-darwin"
  },
  "workbench.activityBar.visible": true,
  "rewrap.doubleSentenceSpacing": true,
  "projects.configPath": "",
  "material-icon-theme.showWelcomeMessage": false,
  "material-icon-theme.folders.theme": "specific",
  "python.autoComplete.addBrackets": true,
  "prettier.tabWidth": 4,
  "workbench.editor.tabSizing": "shrink",
  "python.linting.pylintArgs": [
    "--load-plugins",
    "pylint_django",
    "--disable=C0111",
    "--disable=C0103"
  ],
  "editor.renderIndentGuides": false,
  "vim.disableExtension": false,
  "editor.cursorWidth": 3,
  "zenMode.fullScreen": false,
  "material-icon-theme.showUpdateMessage": false,
  "php.suggest.basic": false,
  "search.location": "panel",
  // "python.pythonPath": "/Users/ivan/anaconda/bin/python",
  // "terminal.integrated.shell.osx": "/usr/local/bin/zsh"
  "bracketPairColorizer.highlightActiveScope": true,
  "editor.matchBrackets": false,
  "bracketPairColorizer.activeScopeCSS": [
    // "backgroundColor : {color}",
    "borderStyle : none none solid none",
    "borderWidth : 2px",
    "borderColor : {color}; opacity: 0.9"
  ],
  "scrolloff.alwaysCenter": true,
  "bracketPairColorizer.showBracketsInGutter": true,
  "bracketPairColorizer.showHorizontalScopeLine": false,
  "bracketPairColorizer.showVerticalScopeLine": false,
  "ruby.intellisense": "rubyLocate",
  "ruby.codeCompletion": "rcodetools",
  "debug.openDebug": "neverOpen",
  "terminal.external.osxExec": "Hyper.app",
  "latex-workshop.message.update.show": false,
  "bracketPairColorizer.consecutivePairColors": [
    "()",
    "[]",
    "{}",
    [
      // "None"
      // "#e1efff"
    ],
    "Red"
  ],
  "latex-workshop.view.pdf.viewer": "tab",
  "files.defaultLanguage": "markdown",
  "window.clickThroughInactive": false,
  "liveServer.settings.donotShowInfoMsg": true,
}